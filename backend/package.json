{
  "name": "primus-os-backend",
  "version": "1.0.0",
  "description": "Primus OS Business Edition - G2R Continuity Operating System Backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "psql -U postgres -d primus_os -f db/schema.sql",
    "db:seed": "ts-node scripts/seed.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "primus-os",
    "continuity",
    "governance",
    "crm",
    "relationship-management"
  ],
  "author": "G2R",
  "license": "PROPRIETARY",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
